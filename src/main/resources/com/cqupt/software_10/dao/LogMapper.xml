<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqupt.software_10.dao.LogMapper">


    <select id="getAllLogs" resultType="com.cqupt.software_10.entity.LogEntity">
        SELECT * FROM software10.log
    </select>


    <select id="selectDataByName" resultType="com.cqupt.software_10.entity.LogEntity">
        SELECT *
        FROM software10.log
        WHERE  ${searchType}  LIKE CONCAT('%', #{name}, '%')

    </select>

    <insert id="myInsert" parameterType="com.cqupt.software_10.entity.LogEntity">
        INSERT INTO software10.log (uid, username, operation, op_time, role)
        VALUES (#{uid}, #{username}, #{operation}, #{opTime}, #{role})
    </insert>

</mapper>